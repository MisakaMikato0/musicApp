/* 水平间距 */
.com_header {
  position: fixed;
  z-index: 1001;
  top: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  width: 100%;
  display: flex;
  padding-top: 25px;
  line-height: 88rpx;
  background: #fff;
}
.com_header .com_header_left {
  flex: 25%;
  height: 100%;
  padding-left: 32rpx;
}
.com_header .com_header_center {
  flex: 40%;
  height: 100%;
  text-align: center;
  color: #111111;
  font-weight: bold;
  font-size: 36rpx;
}
.com_header .com_header_right {
  flex: 25%;
  height: 100%;
  padding-right: 32rpx;
  text-align: right;
  color: #222;
  font-size: 32rpx;
}

